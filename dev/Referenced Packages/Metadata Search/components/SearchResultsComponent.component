    <script type='text/javascript'>
        var j$ = jQuery.noConflict();
        
       /*On ready Function*/
        j$(document).ready(function() {
            hideEmptySections();
            if(j$.cookie("MetadataSearchNotification-{!$User.Id}")=="{!$User.Id}")
                j$('#messageheader').hide();
            else {
                j$('#messageheader').show();
                setTimeout(hideMessageHeader, 15000);
                j$('#messageheaderClose').click(function(){
                    j$.cookie("MetadataSearchNotification-{!$User.Id}", "{!$User.Id}", {expires: new Date(2099, 01, 01, 01, 00, 00)});          
                    j$('#messageheader').hide();                
                });
            }        
        });
        
        var hideMessageHeader = function() {
            j$('#messageheader').hide();
        } 
        
        function sideBarHover(){
            j$('.sideBarLink').hover(
                function(){
                    j$(this).css({"text-decoration":"underline","color":"#2A7FFF"});
                },
                function(){
                    j$(this).css({"text-decoration":"none","color":"grey"});
                }
            );
        }
        
        /** Hide Empty Section **/
        function hideEmptySections(){
            currentSection = '';
            j$('.sidebarLinkWrap').show();
            j$.each(j$('.sidebarLinkWrap'),function(){
                if(j$(this).attr('category-data-count')>0){
                    var resultPanel = j$(this).attr('related-outputpanel');
                    j$('[id$='+resultPanel+']').show();            
                    j$(this).addClass("selectedDiv");
                }
            });
            j$('[id$=searchOne]').hide();
            j$('[id$=searchAll]').attr('Value','Search').button("refresh");
        }

        
        var currentSection = ''; 
        function showSection(secname){           
            j$('.resultBlock').hide('');
            j$('[id$='+secname+']').slideDown();
            j$('[id$=searchOne]').show();
            j$('.sidebarLinkWrap').removeClass("selectedDiv");
            j$('#'+secname+'Anchor').addClass("selectedDiv");
            str= j$('#'+secname+'Anchor').attr('search-category');
            setSearchCategory(str);
            j$('[id$=searchOne]').attr('Value','Search '+str).button("refresh");
            j$('[id$=searchAll]').attr('Value','Search All').button("refresh");
            currentSection=secname;
        }
        
        function showOne(elem){
            showSection(elem);
            j$('.sidebarLinkWrap:not(#'+elem+'Anchor)').slideUp();
            j$('#'+elem+'Anchor').slideDown();
        }
                    
        /** This function will help to call search function on hit of enter **/
        function onEnter(ev)  {
            if (window.event && window.event.keyCode == 13 || ev.which == 13) {
                if(currentSection){
                    doSearchAF();
                }
                else{
                    doSearchAFAll();
                }
                return false;
             } else {
                  return true;
             }
         }
         
        function showNotificationBar(message, duration, bgColor, txtColor, height) {         
            /*set default values*/
            duration = typeof duration !== 'undefined' ? duration : 1500;
            bgColor = typeof bgColor !== 'undefined' ? bgColor : "#F4E0E1";
            txtColor = typeof txtColor !== 'undefined' ? txtColor : "#A42732";
            height = typeof height !== 'undefined' ? height : 40;
            /*create the notification bar div if it doesn't exist*/
            if (j$('#notification-bar').size() == 0) {
                var HTMLmessage = "<div class='notification-message' style='text-align:center; line-height: " + height + "px;'> " + message + " </div>";
                j$('body').prepend("<div id='notification-bar' style='display:none; width:100%; height:" + height + "px; background-color: " + bgColor + "; position: fixed; z-index: 100; color: " + txtColor + ";border-bottom: 1px solid " + txtColor + ";'>" + HTMLmessage + "</div>");
            }
            /*animate the bar*/
            j$('#notification-bar').slideDown(function() {
                setTimeout(function() {
                    j$('#notification-bar').slideUp(function() {});
                }, duration);
            });
        }         
    </script>
